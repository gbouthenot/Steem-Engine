CPP        = $(BCCROOT)\Bin\bcc32

#    Set the compiler options
CCOPTS = -D_NO_VCL -DWINVER=0x0400 -D_WIN32_WINNT=0x0300
CCOPTS = -w  -O2 -OS -Oi -Ov -Og -O -Oc                  $(CCOPTS)
CCOPTS = -jb -j10 -tWM  -r -a4  -6  -vi -d -ff -fp       $(CCOPTS)
CCOPTS = -v- -x -xf -xp- -xd -xs- -H- -y-                $(CCOPTS)

CPPFLAGS= $(CCOPTS) -I$(BCCROOT)\Include

ALL: urarlib.obj

urarlib.obj: 
           $(CPP) $(CPPFLAGS) -c -ourarlib.obj win_urarlib.c
